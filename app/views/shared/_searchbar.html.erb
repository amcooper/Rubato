<div class="" id="search-bar">
  <%= form_tag students_path, method: :get, class:"rounded-input-field" do %>

    <%= text_field_tag :query,
      params[:query],
      class: "rounded-input-field",
      placeholder: "Search for student..."
    %>

    <%= submit_tag "Search", class: "btn-camel" %>
   <% end %>
</div>


